[Unit]
Description=Object Detection Webcam Service
Wants=network-online.target
After=network-online.target

[Container]
Image=quay.io/luisarizmendi/object-detection-stream-manager:x86
AutoUpdate=registry
Ports=5000:5000
Privileged=true
Environment=CAMERA_INDEX=1
Environment=MODEL_NAME=1
Environment=INFERENCE_SERVER_URL=http://localhost:8080
[Service]
Restart=always

[Install]
WantedBy=default.target


